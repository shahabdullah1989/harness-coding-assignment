  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch via Python",
      "type": "python",
      "request": "launch",
      "program": "src/server.py",
      "console": "integratedTerminal",
      "justMyCode": true,
      "serverReadyAction": {
        "pattern": "Uvicorn running on http://0.0.0.0:([0-9]+)",
        "uriFormat": "http://localhost:%s/api/health",
        "action": "openExternally"
      }
    }
  ]
